    <Grid x:Name="MainGrid">
        <Grid.Background>
            <AcrylicBrush TintColor="White" TintOpacity="0.2" />
        </Grid.Background>
        <Grid x:Name="ContentGrid" Visibility="Collapsed">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="40" />
                </Grid.RowDefinitions>
                <Grid x:Name="AppTitleBar" Background="Transparent">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Image
                        Width="26"
                        Height="26"
                        Margin="7,7,0,7"
                        HorizontalAlignment="Left"
                        Source="ms-appx:///Assets/RSSN_Logos/Square44x44Logo.altform-lightunplated_targetsize-48.png" />
                    <TextBlock
                        x:Name="AppTitleTextBlock_1"
                        Margin="40,5,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        FontFamily="{StaticResource HarmonyOSSans}"
                        FontSize="14"
                        Style="{StaticResource CaptionTextBlockStyle}"
                        Text="随机抽取学号 2025" />
                    <TextBlock
                        x:Name="AppTitleTextBlock_2"
                        Grid.ColumnSpan="2"
                        Width="136"
                        Height="17"
                        Margin="40,25,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        FontFamily="{StaticResource HarmonyOSSans}"
                        FontSize="8"
                        Style="{StaticResource CaptionTextBlockStyle}"
                        Text="Randomly Select Student Numbers" />
                </Grid>
                <Grid Margin="180,0,150,0" HorizontalAlignment="Stretch">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <AutoSuggestBox
                        Grid.Column="0"
                        Height="32"
                        MinWidth="150"
                        MaxWidth="350"
                        VerticalAlignment="Center"
                        AutoMaximizeSuggestionArea="True"
                        FontFamily="{StaticResource HarmonyOSSans}"
                        FontSize="14"
                        PlaceholderText="搜索"
                        QueryIcon="Find" />
                    <Button
                        Grid.Column="1"
                        Padding="2"
                        HorizontalAlignment="Right"
                        BorderBrush="{ThemeResource SystemAccentColor}"
                        BorderThickness="1.5"
                        CornerRadius="16">
                        <StackPanel Orientation="Horizontal">
                            <PersonPicture
                                x:Name="SmallClassPicture"
                                Width="32"
                                Height="32"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center" />
                            <TextBlock
                                MaxWidth="100"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Center"
                                FontFamily="{StaticResource HarmonyOSSans}"
                                FontSize="15"
                                Text="{x:Bind ClassNameTextBox.Text, Mode=OneWay}"
                                TextTrimming="CharacterEllipsis" />
                        </StackPanel>
                        <Button.Flyout>

                            <Flyout Placement="Bottom">
                                <StackPanel Width="200">
                                    <PersonPicture
                                        x:Name="BigClassPicture"
                                        Height="72"
                                        HorizontalAlignment="Center" />
                                    <TextBlock
                                        HorizontalAlignment="Center"
                                        FontFamily="{StaticResource HarmonyOSSans}"
                                        FontSize="20"
                                        Text="{x:Bind ClassNameTextBox.Text, Mode=OneWay}" />
                                    <TextBlock
                                        Margin="5"
                                        HorizontalAlignment="Left"
                                        FontFamily="{StaticResource HarmonyOSSans}"
                                        FontSize="14"
                                        Foreground="{ThemeResource TextFillColorSecondaryBrush}"
                                        Text="更改班级名称：" />
                                    <TextBox
                                        x:Name="ClassNameTextBox"
                                        FontFamily="{StaticResource HarmonyOSSans}"
                                        TextChanged="ClassNameTextBox_TextChanged" />
                                    <Button
                                        x:Name="EditPictureButton"
                                        Margin="0,5,0,0"
                                        HorizontalAlignment="Stretch"
                                        Click="EditPictureButton_Click">
                                        <StackPanel Orientation="Horizontal">
                                            <FontIcon FontFamily="{StaticResource SegoeFluentIcons}" Glyph="&#xE70F;" />
                                            <TextBlock
                                                Margin="5,0,0,0"
                                                FontFamily="{StaticResource HarmonyOSSans}"
                                                Text="更改班徽" />
                                        </StackPanel>
                                    </Button>
                                    <Button
                                        x:Name="ManageStudentsButton"
                                        Margin="0,5,0,0"
                                        HorizontalAlignment="Stretch"
                                        Click="ManageStudentsButton_Click">
                                        <StackPanel Orientation="Horizontal">
                                            <FontIcon
                                                FontFamily="{StaticResource SegoeFluentIcons}"
                                                Foreground="{ThemeResource SystemAccentColor}"
                                                Glyph="&#xE7EE;" />
                                            <TextBlock
                                                Margin="5,0,0,0"
                                                FontFamily="{StaticResource HarmonyOSSans}"
                                                Foreground="{ThemeResource SystemAccentColor}"
                                                Text="管理班级学生" />
                                        </StackPanel>
                                    </Button>
                                </StackPanel>
                            </Flyout>
                        </Button.Flyout>
                    </Button>

                </Grid>


            </Grid>

            <Frame
                x:Name="ContentFrame"
                Grid.Row="0"
                Margin="70,45,0,0"
                x:FieldModifier="public"
