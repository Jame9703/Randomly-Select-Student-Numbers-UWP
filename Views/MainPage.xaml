    <Grid x:Name="MainGrid">
        <Grid.Background>
            <AcrylicBrush TintColor="White" TintOpacity="0.2" />
        </Grid.Background>
        <Grid x:Name="ContentGrid" Visibility="Collapsed">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="40" />
                </Grid.RowDefinitions>
                <Grid x:Name="AppTitleBar" Background="Transparent">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Image
                        Width="26"
                        Height="26"
                        Margin="7,7,0,7"
                        HorizontalAlignment="Left"
                        Source="ms-appx:///Assets/RSSN_Logos/Square44x44Logo.altform-lightunplated_targetsize-48.png" />
                    <TextBlock
                        x:Name="AppTitleTextBlock_1"
                        Margin="40,5,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        FontFamily="{StaticResource HarmonyOSSans}"
                        FontSize="14"
                        Style="{StaticResource CaptionTextBlockStyle}"
                        Text="随机抽取学号 2025" />
                    <TextBlock
                        x:Name="AppTitleTextBlock_2"
                        Grid.ColumnSpan="2"
                        Width="136"
                        Height="17"
                        Margin="40,25,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        FontFamily="{StaticResource HarmonyOSSans}"
                        FontSize="8"
                        Style="{StaticResource CaptionTextBlockStyle}"
                        Text="Randomly Select Student Numbers" />
                </Grid>
                <Grid Margin="180,0,150,0" HorizontalAlignment="Stretch">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <AutoSuggestBox
                        Grid.Column="0"
                        Height="32"
                        MinWidth="150"
                        MaxWidth="350"
                        VerticalAlignment="Center"
                        AutoMaximizeSuggestionArea="True"
                        FontFamily="{StaticResource HarmonyOSSans}"
                        FontSize="14"
                        PlaceholderText="搜索"
                        QueryIcon="Find" />
                    <!--<HyperlinkButton
                        x:Name="ClassNameHyperlinkButton"
                        Grid.Column="1"
                        Width="Auto"
                        Height="36"
                        x:FieldModifier="public"
                        BorderBrush="{ThemeResource SystemAccentColorDark2}"
                        BorderThickness="2"
                        Click="ClassNameHyperlinkButton_Click"
                        Content="我的班级"
                        FocusVisualPrimaryBrush="Transparent"
                        FontFamily="{StaticResource HarmonyOSSans}"
                        FontSize="16" />-->
                    <Button
                        Grid.Column="1"
                        Padding="2"
                        HorizontalAlignment="Right"
                        BorderBrush="{ThemeResource SystemAccentColor}"
                        BorderThickness="1.5"
                        CornerRadius="16">
                        <StackPanel Orientation="Horizontal">
                            <PersonPicture
                                Width="28"
                                Height="28"
                                Margin="2,2,2,2"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                ProfilePicture="/Photos/background.jpg" />
                            <TextBlock
                                MaxWidth="100"
                                Margin="0,0,0,0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Center"
                                FontFamily="{StaticResource HarmonyOSSans}"
                                FontSize="15"
                                Text="我的班级"
                                TextTrimming="CharacterEllipsis" />
                        </StackPanel>


                        <Button.Flyout>

                            <Flyout Placement="Bottom">
                                <StackPanel Width="200">
                                    <PersonPicture HorizontalAlignment="Center" ProfilePicture="/Photos/GithubLogo.png" />
                                    <TextBlock
                                        HorizontalAlignment="Center"
                                        FontFamily="{StaticResource HarmonyOSSans}"
                                        FontSize="20"
                                        Text="我的班级" />
                                    <TextBlock
                                        Margin="5"
                                        HorizontalAlignment="Left"
                                        FontFamily="{StaticResource HarmonyOSSans}"
                                        FontSize="14"
                                        Foreground="{ThemeResource TextFillColorSecondaryBrush}"
                                        Text="更改班级名称：" />
                                    <TextBox FontFamily="{StaticResource HarmonyOSSans}" Text="我的班级" />
                                    <Button Margin="0,5,0,0" HorizontalAlignment="Stretch">
                                        <StackPanel Orientation="Horizontal">
                                            <FontIcon FontFamily="{StaticResource SegoeFluentIcons}" Glyph="&#xE70F;" />
                                            <TextBlock
                                                Margin="5,0,0,0"
                                                FontFamily="{StaticResource HarmonyOSSans}"
                                                Text="更改图片" />
                                        </StackPanel>
                                    </Button>
                                    <Button
                                        Margin="0,5,0,0"
                                        HorizontalAlignment="Stretch"
                                        Click="ClassNameHyperlinkButton_Click">
                                        <StackPanel Orientation="Horizontal">
                                            <FontIcon
                                                FontFamily="{StaticResource SegoeFluentIcons}"
                                                Foreground="{ThemeResource SystemAccentColor}"
                                                Glyph="&#xE7EE;" />
                                            <TextBlock
                                                Margin="5,0,0,0"
                                                FontFamily="{StaticResource HarmonyOSSans}"
                                                Foreground="{ThemeResource SystemAccentColor}"
                                                Text="管理班级学生" />
                                        </StackPanel>
                                    </Button>
                                </StackPanel>
                            </Flyout>
                        </Button.Flyout>
                    </Button>

                </Grid>


            </Grid>

            <Frame
                x:Name="ContentFrame"
                Grid.Row="0"
                Margin="70,45,0,0"
                x:FieldModifier="public"
                BorderBrush="Transparent"
                BorderThickness="0"
                CornerRadius="8,0,0,0"
                NavigationFailed="ContentFrame_NavigationFailed">
                <Frame.ContentTransitions>
                    <TransitionCollection>
                        <NavigationThemeTransition />
                    </TransitionCollection>
                </Frame.ContentTransitions>

            </Frame>
