                    CornerRadius="6,6,6,6"
                    FontFamily="{StaticResource HarmonyOSSans}">
                    <FontIcon FontFamily="{StaticResource SegoeFluentIcons}" Glyph="&#xE716;" />
                </ToggleButton>
                <ToggleButton
                    x:Name="NumbersPageButton"
                    Grid.Row="2"
                    Grid.ColumnSpan="2"
                    Width="65"
                    Height="60"
                    HorizontalAlignment="Center"
                    x:FieldModifier="public"
                    Click="NumbersPage_Click"
                    CornerRadius="6,6,6,6"
                    FontFamily="{StaticResource HarmonyOSSans}">
                    <FontIcon FontFamily="{StaticResource SegoeFluentIcons}" Glyph="&#xF4A5;" />
                </ToggleButton>
                <ToggleButton
                    x:Name="CharactersPageButton"
                    Grid.Row="3"
                    Grid.ColumnSpan="2"
                    Width="65"
                    Height="60"
                    HorizontalAlignment="Center"
                    x:FieldModifier="public"
                    Click="CharactersPage_Click"
                    CornerRadius="6,6,6,6"
                    FontFamily="{StaticResource HarmonyOSSans}">
                    <FontIcon FontFamily="{StaticResource SegoeFluentIcons}" Glyph="&#xE164;" />
                </ToggleButton>
                <ToggleButton
                    x:Name="HelpPageButton"
                    Grid.Row="4"
                    Grid.ColumnSpan="2"
                    Width="65"
                    Height="60"
                    HorizontalAlignment="Center"
                    x:FieldModifier="public"
                    Click="HelpPage_Click"
                    CornerRadius="6,6,6,6"
                    FontFamily="{StaticResource HarmonyOSSans}">
                    <FontIcon FontFamily="{StaticResource SegoeFluentIcons}" Glyph="&#xEB68;" />
                </ToggleButton>
                <Grid
                    Height="Auto" />


            </StackPanel>
            <ToggleButton
                    x:Name="SettingsPageButton"
                    Grid.Row="6"
                    Grid.ColumnSpan="2"
                    Width="65"
                    Height="60"
                    x:FieldModifier="Public"
                    Click="SettingsPage_Click"
                    VerticalAlignment="Bottom"
                    CornerRadius="6,6,6,6"
                    FontFamily="{StaticResource HarmonyOSSans}">
                <FontIcon FontFamily="{StaticResource SegoeFluentIcons}" Glyph="&#xE713;" />
            </ToggleButton>
            <Rectangle
                x:Name="MovingRectangle"
                Width="6"
                Height="50"
                Margin="0,50,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Fill="White"
                RadiusX="4"
                RadiusY="4">
                <Rectangle.RenderTransform>
                    <CompositeTransform />
                </Rectangle.RenderTransform>
            </Rectangle>
            <Rectangle
                    x:Name="rectangle"
                    Width="6"
                    Height="50"
                    Margin="0,5,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Bottom"
                    Fill="{ThemeResource AccentFillColorDefaultBrush}"
                    RadiusX="4"
                    RadiusY="4"
                    Visibility="Collapsed">
                <Rectangle.RenderTransform>
                    <CompositeTransform />
                </Rectangle.RenderTransform>
            </Rectangle>
        </Grid>
        <muxc:ProgressRing
            x:Name="LoadProgressRing"
            Width="200"
            Height="200"
            IsActive="True" />
    </Grid>
</Page>
